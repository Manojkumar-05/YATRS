{
  "builds": [
    {
      "src": "Backend/server.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)", 
      "dest": "Backend/server.js"
    },
    {
      "src": "/Frontend/(.*)", 
      "dest": "/"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)", 
      "destination": "/"
    }
  ]
}
